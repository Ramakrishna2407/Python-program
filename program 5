n=[]
for i in range(2):
    input1=input()
    n=n+[input1]
print(n)
if(n[0]==n[1]):
    print("Please enter two different cities")
else:
    k=[]
    k=k+[ "How far is "+n[0]+" from "+n[1] ]+["How far is "+n[1]+" from "+n[0]]+["How is the weather in "+n[0]]+["How is the weather in "+n[1]]
    print(k)
